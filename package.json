{
	"name": "list-task",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"js-lint": "eslint . --ext .js,.ts,.jsx,.tsx",
		"js-lint-fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
		"css-lint": "stylelint src/**/*.{css,scss,sass,less,stylus}",
		"css-lint-fix": "stylelint src/**/*.{css,scss,sass,less,stylus} --fix",
		"format": "prettier --write .",
		"clean-code": "js-lint-fix && yarn run css-lint-fix",
		"commit-lint": "commitlint --edit",
		"test": "jest --watchAll",
		"prepare": "husky install"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.19.4",
		"@babel/preset-react": "^7.18.6",
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.2.1",
		"@types/react": "^18.0.22",
		"@types/react-dom": "^18.0.7",
		"@vitejs/plugin-react": "^2.2.0",
		"babel-jest": "^29.2.2",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"jest": "^29.2.2",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"prop-types": "^15.8.1",
		"stylelint": "^14.14.1",
		"stylelint-config-standard": "^29.0.0",
		"vite": "^3.2.0"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx}": "yarn run js-lint",
		"src/**/*.{css,scss,sass,less,stylus}": "yarn run css-lint"
	}
}
