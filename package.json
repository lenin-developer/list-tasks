{
	"name": "list-task",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"test": "jest --watchAll",
		"js-lint": "eslint . --ext .js,.ts,.jsx,.tsx",
		"js-lint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx",
		"css-lint": "stylelint src/**/*.{css,scss,sass,less,stylus}",
		"css-lint-fix": "stylelint src/**/*.{css,scss,sass,less,stylus} --fix",
		"check-format": "prettier --check {src,__test__}/**/*.{js,jsx,css,scss,md}",
		"format": "prettier --write .",
		"stylelint-check": "stylelint-config-prettier-check",
		"clean-code": "yarn run js-lint-fix && yarn run css-lint-fix && yarn run format",
		"check-code": "yarn run js-lint && yarn run css-lint && prettier --check .",
		"commit-lint": "commitlint --edit",
		"prepare": "husky install",
		"server": "json-server --watch ./src/server/db.json --port 3004"
	},
	"dependencies": {
		"@hookform/resolvers": "^2.9.10",
		"@tanstack/react-query": "^4.19.1",
		"@tanstack/react-query-devtools": "^4.19.1",
		"axios": "^1.2.1",
		"classnames": "^2.3.2",
		"json-server": "^0.17.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.40.0",
		"react-icons": "^4.6.0",
		"react-router-dom": "^6.4.3",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.2.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.2.3",
		"@types/react": "^18.0.22",
		"@types/react-dom": "^18.0.7",
		"@vitejs/plugin-react": "^2.2.0",
		"babel-jest": "^29.3.1",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-jest-dom": "^4.0.3",
		"eslint-plugin-n": "^15.0.0",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^29.3.1",
		"jest-environment-jsdom": "^29.3.1",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"prop-types": "^15.8.1",
		"sass": "^1.56.1",
		"stylelint": "^14.14.1",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-no-px": "^1.0.1",
		"vite": "^3.2.0"
	},
	"lint-staged": {
		"src/**/*.{js,jsx}": "yarn run js-lint",
		"src/**/*.{css,scss}": "yarn run css-lint",
		"*.{js,jsx,css,md}": "yarn run prettier --write"
	}
}
